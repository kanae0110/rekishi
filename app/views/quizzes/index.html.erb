<h1>クイズ</h1>
<%= link_to "問題作成", new_quiz_path %>

 <table>
    <tr> 
        <th>問題</th> 
        
        
    </tr>   
    <% @quizzes.each do |quiz| %>
        <tr>
            <td>
                <%= link_to "クイズ", quiz_path(quiz) %>
            </td>
            <td>
                <%= link_to "編集", edit_quiz_path(quiz) %>
            </td>
            <td>
                <%= link_to "削除", quiz_path(quiz), method: "delete", data: { confirm: '本当に削除しますか?' }, class: "btn btn-danger" %>
            </td>
        </tr>
    <% end %>
</table>
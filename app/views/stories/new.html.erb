<h1>ストーリー新規登録</h1>
<%= form_with model: @story, local: true do |f| %>

    <div class="form-group">
        <label>カテゴリ</label>
        <%= f.text_field :category, class: "form-control" %>
    </div>
    
    <div class="form-group">
        <label>タイトル</label>
        <%= f.text_field :name, class: "form-control" %>
    </div>
    
    <%= f.submit "登録", class: "btn btn-primary" %>
    
    
<% end %>